public class SelectRandomNames {
	public static void main(String[] args) {
		String[] names = { "John", "Peter", "Lucifer", "Oliver", "Jimmy", "Thomson", "William", "James",
				"Lucas", "Benjamin" };
		int result = (int) (Math.random() * names.length);
		String name = names[result];
		System.out.println("Your random name is " + name);
	}

}
